{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "fura unused",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register",
      ],
      "args": [
        "${workspaceFolder}/src/bin/index.ts",
        "invalid-deps",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fura help",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/bin/index.ts",
        "unused",
        "-h",
      ]
    }
  ]
}